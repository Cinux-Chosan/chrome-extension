{
  "manifest_version": 3,
  "name": "Chosan Ext",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "activeTab", "cookies", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
